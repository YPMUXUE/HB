<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss,SSS}, %-5p, [%t], %c{36}#%M:%L - %msg%n"/>
		</Console>
		<File name="log" fileName="log/info.log" append="false">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS}, %-5level, [%t], %class{36}#%M:%L - %msg%n"/>
		</File>
	</Appenders>
	<Loggers>
		<Root level="ERROR">
			<AppenderRef ref="log"/>
		</Root>
	</Loggers>
</Configuration>
